    function searchTermKeyUpHandler(e) {
        if (e.which === '27') {//escape
            e.preventDefault();
            hideSearchResultPopup();
        } else if (e.which === '40') {// down
            e.preventDefault();
            selectTerm();
        } else if (e.which === '38') {//up
